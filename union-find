int par[N],Rank[N];

void initialize()
{
    for(int i=0;i<=n;i++)
    {    
        par[i] = i;
        Rank[i] = 0;
    }
}

int find(int x)
{
    if(par[x]!=x)
        par[x] = find(par[x]);

    return par[x];
}

void Union(int x, int y)
{
    int xpar = find(x);
    int ypar = find(y);

    if(Rank[xpar]<Rank[ypar])
        par[xpar] = ypar;
    else if(Rank[xpar]>Rank[ypar])
        par[ypar] = xpar;
    else
    {
        par[ypar] = xpar;
        Rank[xpar]++;
    }
}

